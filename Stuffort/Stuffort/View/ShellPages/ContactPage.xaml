<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:resx="clr-namespace:Stuffort.Resources"
             x:Class="Stuffort.View.ShellPages.ContactPage"
             Shell.TabBarIsVisible="False">
    <StackLayout>
        <StackLayout>
            <Label Text="Stuffort" HorizontalOptions="Center" FontSize="60" Margin="0,25,0,0" FontFamily="Dosis"
                   TextColor="{StaticResource stuffortBlue}"/>
            <Label x:Name="labelVersion" HorizontalOptions="Center" FontSize="20" TextColor="{StaticResource stuffortBlue}"/>
        </StackLayout>
        <StackLayout>
            <Label Text="{x:Static resx:AppResources.Developer}" Margin="0,40,0,0" HorizontalOptions="Center"/>
            <Label Text="E-mail: bmartin042503@gmail.com" Margin="0,0,0,15" HorizontalOptions="Center"/>
            <Label x:Name="allRightsReserved" HorizontalOptions="Center"/>
        </StackLayout>
    </StackLayout>
</ContentPage>